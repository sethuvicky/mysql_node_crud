{{#each rows}}
<form method="post" action="/edituser/{{this.ID}}" style="padding: 100px;">
<h1>Edit user</h1>
  <div class="form-group" >
    <label for="exampleInputEmail1">Name</label>
    <input type="text" name="name" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp" placeholder="Enter Name" value="{{this.NAME}}">
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Age</label>
    <input type="text" name="age" class="form-control" id="exampleInputPassword1" placeholder="Age" value="{{this.AGE}}">
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">City</label>
    <input type="text" name="city" class="form-control" id="exampleInputPassword1" placeholder="City" value="{{this.CITY}}">
  </div>

  <button type="submit" class="btn btn-primary">Submit</button>
</form>
{{/each}}
